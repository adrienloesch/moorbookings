<ul>
  <% @bookings.each do |booking| %>
    <li><%= link_to booking.mooring.name, mooring_path(booking.mooring) %> from <%= booking.start_date %> to <%= booking.end_date %>
      <%= link_to "Edit", edit_booking_path(booking) %>
      <%= link_to "Delete", booking_path(booking), method: :delete %>
    </li>
  <% end %>
</ul>
