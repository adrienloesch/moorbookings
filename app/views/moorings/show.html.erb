
<div class="container">
  <h1><%= @mooring.name %></h1>
  <%= simple_form_for [@mooring, @booking] do |f| %>
  <%= f.input :start_date, as: :string, required: false, input_html: {class: "flatpickr"} %>
  <%= f.input :end_date, as: :string, required: false, input_html: {class: "flatpickr"} %>

  <%= f.button :submit, "Submit", class: "btn btn-primary", id: "sweet-alert" %>
  <% end %>
</div>
